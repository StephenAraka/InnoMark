{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "react/prop-types": "warn",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": 0,
    "no-debugger": 1,
    "semi": [
      "error",
      "always"
    ],
    "array-bracket-spacing": [
      "warn",
      "never"
    ],
    "camelcase": "warn",
    "max-len": 1,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-plusplus": 0,
    "object-curly-newline": 0,
    "import/no-named-as-default": 0
    }
}